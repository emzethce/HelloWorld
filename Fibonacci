largo = int(input('¿Cuántos elementos de la Serie de Fibonacci quieres ver? '))
def Fibonacci(largo):
    print('\n')  
    fibonacci = [1,1]
    n = 1
    for i in range(1,largo):
        fibonacci.append(int(fibonacci[-1])+ int(fibonacci[-2]))
    for y in fibonacci[0:largo]:
        print (str(n) + '.- '+str(y))
        n += 1
    print('\n'+'Fin de la serie.'+'\n')
Fibonacci(largo)
